pipeline{

    agent any

    stages{

        stage('Continouse Download'){

            steps{
                git branch: 'main', url: 'https://github.com/clemenrance/Practice.git'
            }
        }
        stage('Unit Test'){

            steps{
                sh'mvn test'
            }
        }
        stage('Integration Test'){

            steps{
                sh'mvn verify -DskipUnitTests'
            }
        }
        stage('Code Build'){

            steps{
                sh'mvn clean install'
            }
        }
    }
}